{
  "name": "encompass-pathfinder",
  "displayName": "Encompass Pathfinder",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vbenc",
        "aliases": ["VBENC", "vbenc"],
        "extensions": [".vbenc"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vbenc",
        "scopeName": "source.vbenc",
        "path": "./syntaxes/vbenc.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "vbenc",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "vbenc",
        "path": "./snippets/functions.code-snippets"
      },
      {
        "language": "vbenc",
        "path": "./snippets/encompassObjects.code-snippets"
      }
    ],
    "themes": [
      {
        "label": "My Custom Theme",
        "uiTheme": "vs-dark",
        "path": "./theme/vbenc-theme.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sqlSearch",
          "title": "SQL Search",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sqlSearch": [
        {
          "id": "sqlSearchView",
          "name": "Search Bar"
        }
      ]
    },
    "commands": [
      {
        "command": "sqlSearch.showPanel",
        "title": "Show SQL Search Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.0.0",
    "vscode": "^1.1.37"
  }
}

